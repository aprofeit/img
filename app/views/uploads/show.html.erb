<% content_for :head do %>
  <meta property="og:url" content="https://img.profeit.com/uploads/<%= @upload.token %>">
  <meta property="og:title" content="<%= @upload.image.filename.to_s %>">
  <meta property="og:image" content="<%= url_for(@upload.image) %>">
<% end %>

<center>
  <%= image_tag(@upload.image, class: 'uploaded-image') %>
</center>
